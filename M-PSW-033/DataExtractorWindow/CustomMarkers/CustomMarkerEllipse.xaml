<UserControl x:Class="Demo.WindowsPresentation.CustomMarkers.CustomMarkerEllipse"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="300" Width="300">
    <Grid>
        <Ellipse Name="EllipseMarker" Fill="Transparent" Stroke="Black" Height="10" Width="10" StrokeThickness="1" HorizontalAlignment="Center" VerticalAlignment="Center"></Ellipse>
        <Ellipse Name="EllipsePositionMarker" Fill="Black" Stroke="Black" Height="4" Width="4" StrokeThickness="1" HorizontalAlignment="Center" VerticalAlignment="Center"></Ellipse>
    </Grid>
</UserControl>
